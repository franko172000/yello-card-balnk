{
  "name": "yellow-card-assessment",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "start": "node build/server.js",
    "dev": "nodemon server.ts",
    "db:migrate": "TZ=UTC node_modules/.bin/sequelize db:migrate",
    "db:undo": "TZ=UTC && node_modules/.bin/sequelize db:migrate:undo",
    "db:rollback": "TZ=UTC && node_modules/.bin/sequelize db:migrate:undo:all",
    "test:unit": "NODE_ENV=test npm run db:migrate && jest --config jest.unit.json",
    "test:integration": "NODE_ENV=test jest --detectOpenHandles --testTimeout 30000 --config jest.integration.json"
  },
  "dependencies": {
    "@types/sequelize": "^4.28.15",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "chai-http": "^4.4.0",
    "chance": "^1.1.11",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.1.4",
    "express": "~4.16.1",
    "express-validation": "^4.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "mysql2": "^3.3.5",
    "nodemailer": "^6.9.3",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5",
    "sinon": "^15.1.2",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chance": "^1.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
    "@types/nodemailer": "^6.4.8",
    "@types/sinon": "^10.0.15",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "chai": "^4.3.7",
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "nock": "^13.3.1",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
